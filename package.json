{
  "name": "manage-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "@datastructures-js/priority-queue": "^6.3.2",
    "axios": "^1.8.4",
    "file-saver": "^2.0.5",
    "js-cookie": "^3.0.5",
    "jsencrypt": "^3.3.2",
    "lodash": "^4.17.21",
    "spark-md5": "^3.0.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/css": "^0.6.0",
    "@eslint/js": "^9.24.0",
    "@eslint/json": "^0.11.0",
    "@vitejs/plugin-vue": "^5.2.2",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.1",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.5.1",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{cjs,json}": [
      "prettier --write"
    ],
    "*.{vue,html}": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
